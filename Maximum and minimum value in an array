public class Main {
    public static void main(String[] args) {
        int[] a = {1, 2, 5, 6, 8};
        int large = a[0];
        int min =a[0];

        for (int i = 1; i < a.length; i++) {
            if (a[i] > large) 
            {
                large = a[i];
            }
            else min=a[i];
        }

        System.out.println(large);
        System.out.println(min);
    }
}
