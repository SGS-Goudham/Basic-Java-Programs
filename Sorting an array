public class Main
{
    public static void main(String[] args){
        int[] a = {9,3,8,6};
        int i=0,j=a.length-1;
            while (i < j)
            {   
                    int t = a[i];
                    a[i] = a[j];
                    a[j] = t;
                    i++;
                    j--;
                }
         for (i = 0; i < a.length; i++){
            System.out.println(a[i]);
        }
    }
}
